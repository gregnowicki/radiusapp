
<p>
  <strong>Name:</strong>
  <%= @contact.name %>
</p>

<p>
  <strong>Title:</strong>
  <%= @contact.title %>
</p>

<p>
  <strong>Company:</strong>
  <%= @contact.company %>
</p>

<p>
  <strong>Email:</strong>
  <%= @contact.email %>
</p>

<p>
  <strong>Notes:</strong>
  <%= @contact.notes %>
</p>

<p>
<strong>Goals:</strong>
<table>
<tbody>
<% @contact.goals.each do |goal| %>
        <tr>
          <td><%= goal.title %></td>
          <!-- contact.goal.events -->
        </tr>
      <% end %>
</tbody>
</table>
</p>



<%= link_to 'Edit', edit_contact_path(@contact) %> |
